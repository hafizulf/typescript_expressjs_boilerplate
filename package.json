{
  "name": "typescript_expressjs_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate webApi using Typescript & ExpressJS",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -e ts --ignore ./dist/* --exec \"tsc && tsc-alias && node ./dist/index.js\"",
    "serve": "node ./dist/index.js",
    "db:migration:generate": "sequelize-cli migration:generate --name",
    "db:migrate": "rm -rf dist && tsc && BUILD=true npx sequelize-cli db:migrate",
    "db:migrate:undo": "BUILD=true npx sequelize-cli db:migrate:undo",
    "db:status": "BUILD=true npx sequelize-cli db:migrate:status",
    "db:seeder:generate": "sequelize-cli seed:generate --name",
    "db:seed": "rm -rf dist && tsc && BUILD=true npx sequelize-cli db:seed:all",
    "db:seed:undo": "BUILD=true npx sequelize-cli db:seed:undo:all",
    "db:seed:status": "BUILD=true npx sequelize-cli db:seed:status"
  },
  "keywords": [
    "typescript",
    "expressjs",
    "boilerplate"
  ],
  "author": "Hafizul Furqan",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.3",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cron": "4.3.0",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "express": "4.21.2",
    "express-async-errors": "3.1.1",
    "express-rate-limit": "7.4.1",
    "fs-extra": "11.2.0",
    "http-terminator": "3.2.0",
    "inversify": "6.0.2",
    "jsonwebtoken": "9.0.2",
    "mqtt": "5.10.3",
    "multer": "2.0.2",
    "pg": "8.12.0",
    "redis": "4.7.0",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.37.3",
    "socket.io": "4.8.1",
    "tsc-alias": "1.8.10",
    "uuidv7": "1.0.1",
    "winston": "3.14.2",
    "winston-daily-rotate-file": "5.0.0",
    "zod": "3.23.8"
  },
  "resolutions": {
    "@types/express": "4.17.21"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/body-parser": "1.19.5",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.17",
    "@types/cron": "2.4.3",
    "@types/express": "4.17.21",
    "@types/fs-extra": "11.0.4",
    "@types/jsonwebtoken": "9.0.7",
    "@types/multer": "1.4.5-lts.1",
    "@types/node": "22.3.0",
    "@types/reflect-metadata": "0.1.0",
    "@types/socket.io": "3.0.2",
    "sequelize-cli": "^6.6.3",
    "typescript": "5.5.4"
  }
}
